<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Go.ServerClient.Hibernate.GamesEntity" table="games" schema="gogame">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
            <generator class="native"/>
        </id>
        <property name="blackPlayerId">
            <column name="BlackPlayerID" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="whitePlayerId">
            <column name="WhitePlayerID" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="regDate">
            <column name="reg_date" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>