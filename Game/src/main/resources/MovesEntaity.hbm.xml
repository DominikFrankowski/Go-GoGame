<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Go.ServerClient.Hibernate.MovesEntity" table="moves" schema="gogame">
        <id name="toIgnoreId">
            <column name="ToIgnoreID" sql-type="int(11)"/>
        </id>
        <property name="gameid">
            <column name="GameID" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="move">
            <column name="Move" sql-type="int(11)" not-null="true"/>
        </property>

        <property name="playerIdMove">
            <column name="PlayerIDMove" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="typeOfMove">
            <column name="TypeOfMove" sql-type="enum('ruch', 'pass', 'poddanie sie')" not-null="true"/>
        </property>
        <property name="board">
            <column name="Board" sql-type="varchar(511)" length="511" not-null="true"/>
        </property>
        <property name="regDate">
            <column name="reg_date" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>